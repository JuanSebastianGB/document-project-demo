{
    "plugins": [],
    "source": {
        "include":["src"],
        "includePattern": ".(js|jsx)$",
        "excludePattern": "(node_modules|docs)"
    },
    "templates":{
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts":{
        "readme": "./README.md",
        "template": "node_modules/clean-jsdoc-theme",
        "recurse": true,
        "verbose": true,
        "destination":"./docs",
        "theme_opts": {
            "theme": "dark"
        },
        "tutorials":"./tutorials"
    }
}